###  http://data.open.ac.uk/ontology/red/oberservationProducedBy
red:oberservationProducedBy rdf:type owl:ObjectProperty .


###  http://data.open.ac.uk/ontology/red/textInvolved
red:textInvolved rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty led:is_reported_in .


###  http://led.kmi.open.ac.uk/term/has_environment
led:has_environment rdf:type owl:ObjectProperty .


###  http://led.kmi.open.ac.uk/term/is_reported_in
led:is_reported_in rdf:type owl:ObjectProperty .


###  http://readit-project/ontology/includes
readit:includes rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  http://data.open.ac.uk/ontology/red/Experience_Listener
red:Experience_Listener rdf:type owl:Class ;
                        owl:equivalentClass led:ListeningExperience .


###  http://led.kmi.open.ac.uk/term/Experience
led:Experience rdf:type owl:Class ;
               rdfs:subClassOf readit:Engagement .


###  http://led.kmi.open.ac.uk/term/ListeningExperience
led:ListeningExperience rdf:type owl:Class ;
                        rdfs:subClassOf [ rdf:type owl:Restriction ;
                                          owl:onProperty readit:includes ;
                                          owl:someValuesFrom :AuralObservation
                                        ] .


###  http://modellingdh.github.io/ont/led-ceo#AuralObservation
:AuralObservation rdf:type owl:Class ;
                  rdfs:subClassOf readit:Observation ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty red:oberservationProducedBy ;
                                    owl:someValuesFrom mo:Performance
                                  ] ;
                  <http://purl.org/dc/terms/description> "An observation generated from a reflection on a sensory stimulus of the ear."@en ;
                  rdfs:comment "Added in alignment"@en ;
                  rdfs:label "aural observation"@en .


###  http://purl.org/ontology/mo/Performance
mo:Performance rdf:type owl:Class .


###  http://readit-project/ontology/Engagement
readit:Engagement rdf:type owl:Class .


###  http://readit-project/ontology/Observation
readit:Observation rdf:type owl:Class .


